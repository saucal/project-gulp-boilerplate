{
  "name": "project-name",
  "description": "Project Description",
  "version": "0.0.1",
  "main": "gulpfile.js",
  "dependencies": {
    "@wordpress/eslint-plugin": "^4.1.0",
    "@wordpress/scripts": "^7.2.0",
    "@wordpress/stylelint-config": "^19.0.1",
    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.3",
    "glob": "^7.1.6",
    "gulp": ">=4.0.0",
    "gulp-add-src": "^1.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-checktextdomain": "^2.2.0",
    "gulp-cheerio": ">=0.6.3",
    "gulp-compass": ">=2.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^5.1.0",
    "gulp-load-plugins": ">=1.5.0",
    "gulp-plumber": ">=1.2.1",
    "gulp-rename": ">=1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "gulp-vinyl-zip": "^2.1.2",
    "gulp-wp-pot": "^2.3.4",
    "merge-stream": "^1.0.1",
    "npm-run": "^5.0.1",
    "path": ">=0.12.7",
    "postcss": "^8.2.14",
    "postcss-clean": "^1.2.2",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.12",
    "semver": "^6.0.0",
    "underscore": "^1.9.1"
  },
  "assets": {
    "css": {
      "src": "assets/source/sass/",
      "out": "assets/css/"
    }
  },
  "scripts": {
    "presass": "for d in $npm_package_folders; do rm -rf \"${d}/${npm_package_assets_css_out}\"; done;",
    "sass": "for d in $npm_package_folders; do export local_dir; local_dir=\"${d}/${npm_package_assets_css_src}\"; export local_out; local_out=\"${d}/${npm_package_assets_css_out}\"; npm run sass:unminified; npm run sass:minified ; done;",
    "sass:unminified": "sass --no-source-map --style=expanded $local_dir:$local_out && postcss \"${local_out}\" --base \"${local_out}\" --dir \"${local_out}\" --no-map --use autoprefixer",
    "sass:minified": "postcss \"${local_out}\" --base \"${local_out}\" --dir \"${local_out}\" --no-map --use postcss-clean --ext '.min.css'",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "format:js": "wp-scripts format-js"
  },
  "browserslist": [
    "last 5 versions",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:@wordpress/eslint-plugin/es5"
    ],
    "env": {
      "browser": true,
      "commonjs": true,
      "node": false
    }
  },
  "stylelint": {
    "extends": [
      "@wordpress/stylelint-config/scss"
    ],
    "rules": {
      "no-descending-specificity": null
    }
  },
  "keywords": [
    "wordpress",
    "wordpress theme",
    "wordpress plugin"
  ],
  "author": "SAU/CAL",
  "license": "ISC",
  "folders": "wp-content/plugins/project-plugin wp-content/themes/project-theme"
}
