{
  "name": "project-name",
  "description": "Project Description",
  "version": "0.0.1",
  "main": "gulpfile.js",
  "dependencies": {
    "@wordpress/eslint-plugin": "^4.1.0",
    "@wordpress/scripts": "^15.0.1",
    "@wordpress/stylelint-config": "^19.0.1",
    "browser-sync": "^2.26.14",
    "concurrently": "^6.0.2",
    "glob": "^7.1.6",
    "gulp": ">=4.0.0",
    "gulp-add-src": "^1.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-checktextdomain": "^2.2.0",
    "gulp-cheerio": ">=0.6.3",
    "gulp-compass": ">=2.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^5.1.0",
    "gulp-load-plugins": ">=1.5.0",
    "gulp-plumber": ">=1.2.1",
    "gulp-rename": ">=1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "gulp-vinyl-zip": "^2.1.2",
    "gulp-wp-pot": "^2.3.4",
    "lerna": "^4.0.0",
    "merge-stream": "^1.0.1",
    "npm-run": "^5.0.1",
    "path": ">=0.12.7",
    "semver": "^6.0.0",
    "underscore": "^1.9.1"
  },
  "domain": "project-domain.test",
  "scripts": {
    "prepare": "lerna bootstrap --hoist",
    "build": "lerna run build --parallel",
    "watch": "lerna run watch --parallel",
    "browsersync": "concurrently --raw \"npm:watch\" \"npm:browsersync:serve\"",
    "browsersync:list": "files=\"$(lerna exec --concurrency 1 --no-prefix -- npm run browsersync:list --if-present --silent)\"; echo \"${files/$'\n'/, }\"",
    "browsersync:serve": "browser-sync start --no-open --proxy \"$npm_package_domain\" --files \"$(npm run browsersync:list --silent)\"",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "format:js": "wp-scripts format-js"
  },
  "browserslist": [
    "last 5 versions",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:@wordpress/eslint-plugin/es5"
    ],
    "env": {
      "browser": true,
      "commonjs": true,
      "node": false
    }
  },
  "stylelint": {
    "extends": [
      "@wordpress/stylelint-config/scss"
    ],
    "rules": {
      "no-descending-specificity": null
    }
  },
  "keywords": [
    "wordpress",
    "wordpress theme",
    "wordpress plugin"
  ],
  "author": "SAU/CAL",
  "license": "ISC"
}
